    var response = sforce.apex.execute(&apos;Ownership_Service&apos;, &apos;releaseTemporaryOwnership&apos;,{accountString : JSON.stringify(accountObj),adminUserId : &apos;{!$Setup.SME_Admin__c.User_Record_ID__c}&apos;});

