        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/34.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/34.0/apex.js&quot;)}
if({!Account.Allow_Request_Ownership__c}) {
var accountObj = { 
		Id:&apos;{!Account.Id}&apos;, 
		OwnerId:&apos;{!Account.OwnerId}&apos;, 
		Port_Pool__c:&apos;{!Account.Port_Pool__c}&apos;, 
