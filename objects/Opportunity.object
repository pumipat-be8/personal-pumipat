    <webLinks>
        <fullName>Close_Won</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Close Won</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/33.0/connection.js&quot;)}

var query_result = sforce.connection.query(&quot;SELECT Id, Status__c FROM Opportunity_Product__c WHERE Opportunity__c = &apos;{!Opportunity.Id}&apos;&quot;).getArray(&quot;records&quot;);

var hasWaiting = false;
var hasSetUp = false;
var hasCancel = false;

for (var i = -1; ++i &lt; query_result.length;) {
	var oppProduct = query_result[i];
	if (oppProduct.Status__c == &apos;Waiting for setup&apos;) {
		hasWaiting = true;
		break;
	} else if (oppProduct.Status__c == &apos;Setup&apos;) {
		hasSetUp = true;
	} else if (oppProduct.Status__c == &apos;Cancel&apos;) {
		hasCancel = true;
	}
}

if (!hasWaiting &amp;&amp; hasSetUp) {
	var oppObj = new sforce.SObject(&quot;Opportunity&quot;);
	oppObj.Id = &apos;{!Opportunity.Id}&apos;;

//    var recordType = sforce.connection.query(&quot;SELECT Id FROM RecordType WHERE DeveloperName = &apos;Closed_Credit_Opportunity&apos;&quot;).getArray(&quot;records&quot;);
//    oppObj.RecordTypeId = recordType[0].Id;
	oppObj.IsCloseWon__c = true;

	if (hasCancel) {
		oppObj.StageName = &quot;Partial Loan Setup&quot;;
	} else {
		oppObj.StageName = &quot;All Loan Setup&quot;;
	}

	var result = sforce.connection.update([oppObj]); 

	console.log(&apos;result&apos;+result); 
	if (result[0].success==&apos;false&apos;) { 
		alert(result[0].errors.message); 
	} else { 
		location.reload(true); 
	}
} else {
	alert(&quot;{!$Label.CANNOT_CLOSE_WON_OPP}&quot;);
}</url>
    </webLinks>
    <webLinks>
        <fullName>Closed_Lost</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Closed Lost</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/32.0/connection.js&quot;)} 

var element = document.getElementById(&apos;lostReason&apos;); 
if (typeof(element) === &quot;undefined&quot; || element == null){ 

var option_val = &apos;&apos;; 
var result = sforce.connection.describeSObject(&quot;Opportunity&quot;); 
for (var i=0,len = result.fields.length; i &lt; len; i++) { 
var field = result.fields[i]; 
if(field.name == &apos;Lost_reason__c&apos;){ 
var oPicklistVals = field.picklistValues; 
for (var b=0; b&lt;oPicklistVals.length; b++) { 
console.log(oPicklistVals[b]); 
option_val +=&apos;&lt;option&apos;+((b==0)?&apos; selected=&quot;selected&quot;&apos;:&apos;&apos;)+&apos; value=&quot;&apos;+oPicklistVals[b].value+&apos;&quot;&gt;&apos;+oPicklistVals[b].label+&apos;&lt;/option&gt;&apos;; 
} 
break; 
} 
} 

document.body.innerHTML += &apos;&lt;div id=&quot;lostReason&quot; style=&quot; background: #fff none repeat scroll 0 0;border-radius: 5px;box-shadow: 0 0 4px rgba(0, 0, 0, 0.7);display: none;padding: 30px;&quot;&gt;&lt;table&gt;&lt;tr&gt;&lt;td&gt;Lost Reason :&lt;/td&gt;&lt;td id=&quot;sel-ele&quot;&gt;&lt;select id=&quot;lostReasonSelect&quot;&gt;&apos;+option_val+&apos;&lt;/select&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr id=&quot;other_tr&quot;&gt;&lt;td&gt;หมายเหตุ :&lt;/td&gt;&lt;td&gt;&lt;input id=&quot;other_input&quot; type=&quot;text&quot; class=&quot;txt&quot; /&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td&gt;&lt;/td&gt;&lt;td&gt;&lt;button class=&quot;btn&quot; id=&quot;save_button&quot;&gt;Save&lt;/button&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/table&gt;&lt;/div&gt;&lt;div id=&quot;lean_overlay&quot; style=&quot;position: fixed;z-index:100;top: 0px;left: 0px;height:100%;width:100%;background: #000;display: none;&quot;&gt;&lt;/div&gt;&apos;; 

document.getElementById(&apos;save_button&apos;).onclick = function(){ 
this.disabled = true; 
var lostReasonVal = document.getElementById(&apos;lostReasonSelect&apos;).value; 
var newRecords = []; 

var opp = new sforce.SObject(&quot;Opportunity&quot;); 
opp.id = &quot;{!Opportunity.Id}&quot;; 
opp.StageName = &quot;Closed Lost&quot;; 
opp.Remark__c = document.getElementById(&apos;other_input&apos;).value; 
opp.Lost_reason__c = lostReasonVal; 

newRecords.push(opp); 

result = sforce.connection.update(newRecords); 
if (result[0].getBoolean(&apos;success&apos;)) { 
alert(&apos;Closed Lost สำเร็จ&apos;); 
location.reload(); 

} else { 
alert(&apos;An error occurred updating this Opportunity:\n\n&apos; + result[0].errors.message); 
this.disabled = false; 
} 
}; 

document.getElementById(&apos;lean_overlay&apos;).addEventListener(&quot;click&quot;, function(e) { 
document.getElementById(&apos;lean_overlay&apos;).style.display = &quot;none&quot;; 
document.getElementById(&apos;lostReason&apos;).style.display = &quot;none&quot;; 

}); 
} 
var query_result = sforce.connection.query(&quot;SELECT Id FROM Opportunity_Product__c WHERE Opportunity__c =&apos;{!Opportunity.Id}&apos; AND Status__c!=&apos;Cancel&apos;&quot;).getArray(&quot;records&quot;); 
if(query_result.length &gt; 0) alert(&quot;Some Products status still not cancel&quot;); 
else{ 
var confirmation = window.confirm(&quot;Do you want to close Opportunity as Lost?&quot;); 

if (confirmation == true){ 

document.getElementById(&apos;other_input&apos;).value = &apos;&apos;; 

var modal_id = &apos;lostReason&apos;; 

var modal_width = 560; 
document.getElementById(&apos;lean_overlay&apos;).style.display = &apos;block&apos;; 
document.getElementById(&apos;lean_overlay&apos;).style.opacity = &apos;0.5&apos;; 

document.getElementById(&apos;lostReason&apos;).style.display = &apos;block&apos;; 
document.getElementById(&apos;lostReason&apos;).style.position = &apos;fixed&apos;; 
document.getElementById(&apos;lostReason&apos;).style.opacity = &apos;1&apos;; 
document.getElementById(&apos;lostReason&apos;).style.zIndex = &apos;11000&apos;; 
document.getElementById(&apos;lostReason&apos;).style.left = &apos;50%&apos;; 
document.getElementById(&apos;lostReason&apos;).style.marginLeft = &apos;-&apos;+(modal_width/2)+&quot;px&quot;; 
document.getElementById(&apos;lostReason&apos;).style.top = &apos;100px&apos;; 
} 
}</url>
