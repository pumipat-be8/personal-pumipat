    <validationRules>
        <fullName>AttendeeConfirmationDate_must_be_earlier</fullName>
        <active>true</active>
        <errorConditionFormula>AND(OR(RecordType.DeveloperName = &quot;SME_Event&quot;,RecordType.DeveloperName = &quot;SME_Event_Non_Targeted&quot;), Attendee_Confirmation_Date__c &gt;  Start_Date_Time__c)</errorConditionFormula>
        <errorDisplayField>Attendee_Confirmation_Date__c</errorDisplayField>
        <errorMessage>Attendee Confirmation Date must be earlier than Event Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Active_and_Start_Date</fullName>
        <active>true</active>
        <errorConditionFormula>RecordType.DeveloperName = &apos;SME_Lead_Campaign&apos; &amp;&amp; ISPICKVAL(Status, &apos;Active&apos;) &amp;&amp; (ISBLANK(StartDate) || StartDate &gt; TODAY())</errorConditionFormula>
        <errorDisplayField>StartDate</errorDisplayField>
        <errorMessage>Start Date should equal less than or equals Today, to active the Campaign.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Effective_Date_Cannot_Less_Than_1_Year</fullName>
        <active>true</active>
        <errorConditionFormula>Effective_Date__c -  DATE((YEAR(StartDate) - 1), MONTH(StartDate), DAY(StartDate)) &lt; 0</errorConditionFormula>
        <errorDisplayField>Effective_Date__c</errorDisplayField>
        <errorMessage>Effective Date Cannot Backward More Than 1 Year</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>End_Date_Time_must_be_later</fullName>
        <active>true</active>
        <errorConditionFormula>AND( OR(RecordType.DeveloperName = &quot;SME_Event&quot;,RecordType.DeveloperName = &quot;SME_Event_Non_Targeted&quot;), End_Date_Time__c  &lt;  Start_Date_Time__c)</errorConditionFormula>
        <errorDisplayField>End_Date_Time__c</errorDisplayField>
        <errorMessage>Event End Time must be later than Start Time</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>End_Date_must_be_later_than_Start_Date</fullName>
        <active>true</active>
        <errorConditionFormula>AND( RecordType.DeveloperName = &quot;SME_Campaign&quot; ,  EndDate   &lt;   StartDate )</errorConditionFormula>
        <errorDisplayField>EndDate</errorDisplayField>
        <errorMessage>Lead/Campaign End Date must be later than Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Please_select_Reason_for_Cancellation</fullName>
        <active>true</active>
        <description>when Event status is equal to Cancel, Event team need to specify a reason for cancellation.</description>
        <errorConditionFormula>AND(OR(RecordType.DeveloperName = &quot;SME_Event&quot;,RecordType.DeveloperName = &quot;SME_Event_Non_Targeted&quot;) ,TEXT(Status) = &quot;Cancel&quot;, ISBLANK(Reason_for_Cancellation__c))</errorConditionFormula>
        <errorDisplayField>Reason_for_Cancellation__c</errorDisplayField>
        <errorMessage>Please provide reason for cancellation</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Please_select_Reason_for_Change</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
OR(
RecordType.DeveloperName = &quot;SME_Event&quot;,RecordType.DeveloperName = &quot;SME_Event_Non_Targeted&quot;
)
, ((ISCHANGED(Start_Date_Time__c)||ISCHANGED(End_Date_Time__c)||ISCHANGED(Venue__c))&amp;&amp; ISBLANK(Reason_for_Change__c)))</errorConditionFormula>
        <errorDisplayField>Reason_for_Change__c</errorDisplayField>
        <errorMessage>Please provide reason for change</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PreventChangeEventStatusBwd</fullName>
        <active>true</active>
        <description>Prevent user to change the status of the event record backward e.g. change from cancel to active</description>
        <errorConditionFormula>ISCHANGED(Status) &amp;&amp; (OR(RecordType.DeveloperName = &quot;SME_Event&quot;,RecordType.DeveloperName = &quot;SME_Event_Non_Targeted&quot;)) &amp;&amp;  PRIORVALUE(Status_Order_Flag__c)&gt;Status_Order_Flag__c</errorConditionFormula>
        <errorDisplayField>Status</errorDisplayField>
        <errorMessage>Cannot change status backward</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Required_LeadType</fullName>
        <active>true</active>
        <errorConditionFormula>AND(RecordType.DeveloperName=&quot;SME_Lead_Campaign&quot;,
LEN(TEXT(Type__c))=0)</errorConditionFormula>
        <errorDisplayField>Type__c</errorDisplayField>
        <errorMessage>You must enter a value</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restrict_Change_of_Event_Name</fullName>
        <active>true</active>
        <errorConditionFormula>OR(RecordType.DeveloperName = &quot;SME_Event&quot;,RecordType.DeveloperName = &quot;SME_Event_Non_Targeted&quot;)&amp;&amp; 
ISCHANGED( Name ) &amp;&amp; 
$Setup.User_Management__c.Allow_Event_Name_Change__c  = False</errorConditionFormula>
        <errorMessage>Event Name can only be updated by authorized user</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Start_Date_Time_Cannot_Less_Than_Today</fullName>
        <active>true</active>
        <description>Event Start Date Time should equals or more than Today</description>
        <errorConditionFormula>AND(

OR(RecordType.DeveloperName = &quot;SME_Event&quot;,RecordType.DeveloperName = &quot;SME_Event_Non_Targeted&quot;),

if(ISCHANGED(Start_Date_Time__c)||isnew(),DATEVALUE(Start_Date_Time__c)&lt; TODAY(),false))</errorConditionFormula>
        <errorDisplayField>Start_Date_Time__c</errorDisplayField>
        <errorMessage>Event Start Date Time should equals or more than Today</errorMessage>
    </validationRules>
