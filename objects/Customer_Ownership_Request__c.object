    <validationRules>
        <fullName>CannotRequestPoolWhenCustSMEAndSMEAdmin</fullName>
        <active>true</active>
        <description>Cannot request customer type pool when customer segment is in SME segment and owner is SME-Admin</description>
        <errorConditionFormula>CIS_ID__r.Owner_is_SME_Admin__c &amp;&amp; 
ispickval(Request_Ownership_Type__c, &quot;Pool (Temporary)&quot;) &amp;&amp; 
(CIS_ID__r.Primary_Segment_Code__c = &quot;SM&quot; || 
CIS_ID__r.Primary_Segment_Code__c = &quot;LM&quot; || 
CIS_ID__r.Primary_Segment_Code__c = &quot;UM&quot; || 
CIS_ID__r.Primary_Segment_Code__c = &quot;SR&quot; || 
CIS_ID__r.Primary_Segment_Code__c = &quot;LR&quot; || 
CIS_ID__r.Primary_Segment_Code__c = &quot;UR&quot;) &amp;&amp; 
RecordType.DeveloperName = &quot;Request_Ownership_Port&quot; &amp;&amp; 
ISPICKVAL(Approval_Status__c, &quot;Open&quot;)</errorConditionFormula>
        <errorDisplayField>Request_Ownership_Type__c</errorDisplayField>
        <errorMessage>Cannot request customer type pool when customer segment is in SME segment and owner is SME-Admin</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_required_fields</fullName>
        <active>true</active>
        <errorConditionFormula>AND( NOT(ISNEW()) , ISPICKVAL( Approval_Status__c, &quot;Open&quot;) , OR(ISBLANK( Reason__c ) , ISPICKVAL(Request_Ownership_Type__c, &quot;&quot;)) ,  OR( RecordType.DeveloperName = &quot;Request_Ownership_Pool_Temporary&quot; , RecordType.DeveloperName = &quot;Request_Ownership_Port&quot; , RecordType.DeveloperName = &quot;Customer_Ownership_Request_For_approval&quot;))</errorConditionFormula>
        <errorMessage>Please input Reason and select Request Ownership Type</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Not_allow_to_change_ownership</fullName>
        <active>true</active>
        <description>User cannot change owner of Customer Ownership Request record</description>
        <errorConditionFormula>ISCHANGED(OwnerId )</errorConditionFormula>
        <errorMessage>ไม่อนุญาตให้ทำการแก้ไข Owner</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Refer_Out_Area_is_required</fullName>
        <active>false</active>
        <errorConditionFormula>TEXT( Refer_Out_Reason__c ) = &quot;Wrong Area&quot; &amp;&amp;
ISBLANK( Refer_Out_Area__c )</errorConditionFormula>
        <errorDisplayField>Refer_Out_Area__c</errorDisplayField>
        <errorMessage>Refer Out Area is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Refer_Out_Segment_is_required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(OR(
ISPICKVAL(Refer_Out_Reason__c, &quot;Other Segment&quot;),
ISPICKVAL(Refer_Out_Reason__c, &quot;Wrong Area&quot;)),
ISBLANK(TEXT(Refer_Out_Segment__c)))</errorConditionFormula>
        <errorDisplayField>Refer_Out_Segment__c</errorDisplayField>
        <errorMessage>Refer Out Segment is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Refer_Out_Team_is_required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
OR(
ISPICKVAL(Refer_Out_Reason__c , &quot;Other Team&quot;),
ISPICKVAL(Refer_Out_Reason__c , &quot;Wrong Area&quot;)),
ISBLANK( Refer_Out_Team__c))</errorConditionFormula>
        <errorDisplayField>Refer_Out_Team__c</errorDisplayField>
        <errorMessage>Refer Out Team is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Refer_Out_to_TM_Pool_selected_by_RM</fullName>
        <active>true</active>
        <errorConditionFormula>AND(TEXT( Refer_Out_Reason__c ) = &quot;TM Pool&quot;, AND($Profile.Name &lt;&gt; &quot;SME-RM&quot;, $Profile.Name &lt;&gt; &quot;SME-TM&quot;))</errorConditionFormula>
        <errorDisplayField>Refer_Out_Reason__c</errorDisplayField>
        <errorMessage>Refer Out to TM Pool can only be selected by RM</errorMessage>
    </validationRules>
